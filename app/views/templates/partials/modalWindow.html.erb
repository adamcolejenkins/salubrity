<div tabindex="-1" role="dialog" class="md-modal md-effect-12 full-color" ng-class="{in: animate}" ng-style="{'z-index': 1050 + index*10, display: 'block', perspective: '1300px'}" ng-click="close($event)">
<div class="modal-dialog" ng-class="{'modal-sm': size == 'sm', 'modal-lg': size == 'lg'}"><div class="modal-content" ng-transclude></div></div>
</div>